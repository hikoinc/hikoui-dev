{
  "name": "@hikoui/storybook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "storybook build",
    "clean": "rimraf storybook-static",
    "dev": "pnpm storybook",
    "eslint": "eslint . --max-warnings 0",
    "eslint:fix": "eslint . --fix --max-warnings 0",
    "generate-component": "turbo gen react-component",
    "sort-package-json": "pnpm exec sort-package-json",
    "sort-package-json:check": "pnpm exec sort-package-json --check",
    "storybook": "storybook dev -p 6006",
    "typescheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@hikoui/eslint-config-hikoui": "workspace:*",
    "@hikoui/typescript-config-hikoui": "workspace:*",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.27.0",
    "eslint-plugin-storybook": "^0.12.0",
    "playwright": "^1.52.0",
    "sort-package-json": "^3.0.0",
    "storybook": "^8.6.14",
    "typescript": "5.8.2",
    "vitest": "^3.1.4"
  }
}
